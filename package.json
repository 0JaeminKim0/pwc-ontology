{
  "name": "webapp",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:sandbox": "wrangler pages dev dist --ip 0.0.0.0 --port 3000",
    "build": "vite build",
    "start": "node railway-server.js",
    "start:railway": "PORT=${PORT:-3000} node railway-server.js",
    "start:express": "node server-express.js",
    "build:inline": "node build-inline.js",
    "preview": "wrangler pages dev dist",
    "deploy": "npm run build && wrangler pages deploy dist",
    "deploy:prod": "npm run build && wrangler pages deploy dist --project-name pwc-ontology",
    "deploy:railway": "npm run build",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "clean-port": "fuser -k 3000/tcp 2>/dev/null || true",
    "test": "curl http://localhost:3000",
    "git:commit": "git add . && git commit -m"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.2",
    "@railway/cli": "^4.6.3",
    "@types/d3": "^7.4.3",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/three": "^0.180.0",
    "axios": "^1.11.0",
    "canvas": "^3.2.0",
    "d3": "^7.9.0",
    "express": "^4.21.2",
    "framer-motion": "^12.23.12",
    "gsap": "^3.13.0",
    "hono": "^4.9.6",
    "pdf-poppler": "^0.2.1",
    "pdf2pic": "^3.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sharp": "^0.34.3",
    "three": "^0.180.0"
  },
  "devDependencies": {
    "@hono/vite-build": "^1.7.0",
    "@hono/vite-dev-server": "^0.18.2",
    "vite": "^6.3.5",
    "wrangler": "^4.4.0"
  }
}
